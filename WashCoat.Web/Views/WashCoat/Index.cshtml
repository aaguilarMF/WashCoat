<div>
    <!-- Button trigger modal -->
    <div class="row">
        <div class="col-md-10"></div>
        <div class="col-md-2">
            <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#myModal">
                Add New Data
            </button>
        </div>
    </div>
    <div class="gridStyle" ui-grid="gridOptions" ui-grid-pinning></div>
</div>
<div ng-controller="AddWashCoatController">
    <!-- Modal -->
    <div class="modal" data-dismiss="modal" id="myModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Modal title</h5>
                    <button type="button" class="close" id="dismissModal" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div ng-repeat="(label, scopeElement) in dataToSubmit">
                        <div class="row">
                            <div class="col-md-3"  >
                                <label style="text-align:right">{{label}}:</label>
                            </div>
                            <div class="col-md-9">
                                <input type="text" ng-model="scopeElement.val" />
                            </div>
                        </div>
                    </div>
                </div>
                    
                <div class="modal-footer">
                    <button  type="button" class="btn btn-secondary" data-dismiss="modal" >Close</button>
                    <button type="button" class="btn btn-primary" ng-click="addWashCoat()">Save changes</button>
                </div>
            </div>
        </div>
    </div>
</div>




